<h1>Editing attachment</h1>

<% form_for [@project, @attachment ], :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p><%= f.submit 'Update' %></p>
<% end %>

<%= link_to 'Show', @attachment %> |
<%= link_to 'Back', attachments_path %>
